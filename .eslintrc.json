{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "import/extensions": "off",
      "no-console": "off",
      "no-alert":"off",
      "no-param-reassign": "off"
    }
}
