{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "import/extensions": "off",
      "no-console": "off",
      "no-alert":"off"
      /* "import/no-extraneous-dependencies": ["error", { "devDependencies": true }] */
    }
}
